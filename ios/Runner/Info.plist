<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>NSContactsUsageDescription</key>
		<string>Sellout uses your contacts to help you invite friends to the marketplace and find people you know who are already using the app</string>
		<key>NSCameraUsageDescription</key>
		<string>Sellout uses your camera to take photos of marketplace products, capture profile pictures, and share images in chat conversations</string>
		<key>NSMicrophoneUsageDescription</key>
		<string>Sellout uses your microphone to record voice messages in chat conversations, audio notes for marketplace listings, and voice reviews for products and services</string>
		<key>NSPhotoLibraryUsageDescription</key>
		<string>Sellout accesses your photo library to upload marketplace product images, select profile pictures, and share photos in chat conversations</string>
		<key>NSPhotoLibraryAddUsageDescription</key>
		<string>This app saves photos to your gallery.</string>
		<key>NSAppTransportSecurity</key>
		<dict>
			<key>NSAllowsArbitraryLoads</key>
			<false/>
		</dict>
		<key>UIApplicationSceneManifest</key>
		<dict>
			<key>UIApplicationSupportsMultipleScenes</key>
			<false/>
		</dict>
		<key>LSApplicationQueriesSchemes</key>
		<array>
			<string>stripe</string>
			<string>sms</string>
			<string>tel</string>
		</array>
		<key>CADisableMinimumFrameDurationOnPhone</key>
		<true/>
		<key>CFBundleDevelopmentRegion</key>
		<string>$(DEVELOPMENT_LANGUAGE)</string>
		<key>CFBundleDisplayName</key>
		<string>Sellout</string>
		<key>CFBundleExecutable</key>
		<string>$(EXECUTABLE_NAME)</string>
		<key>CFBundleIdentifier</key>
		<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
		<key>CFBundleInfoDictionaryVersion</key>
		<string>6.0</string>
		<key>CFBundleLocalizations</key>
		<array>
			<string>en</string>
		</array>
		<key>CFBundleName</key>
		<string>sellout</string>
		<key>CFBundlePackageType</key>
		<string>APPL</string>
		<key>CFBundleShortVersionString</key>
		<string>$(FLUTTER_BUILD_NAME)</string>
		<key>CFBundleSignature</key>
		<string>????</string>
		<key>CFBundleURLTypes</key>
		<array>
			<dict>
				<key>CFBundleTypeRole</key>
				<string>Editor</string>
				<key>CFBundleURLSchemes</key>
				<array>
					<string>selloutweb.com</string>
				</array>
			</dict>
		</array>
		<key>CFBundleVersion</key>
		<string>$(FLUTTER_BUILD_NUMBER)</string>
		<key>LSRequiresIPhoneOS</key>
		<true/>
		<key>NSAppleMusicUsageDescription</key>
		<string>Sellout integrates with Apple Music to let you share your favorite songs with friends in chat conversations and showcase music preferences on your profile</string>
		<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
		<string>Sellout uses your location to show nearby marketplace listings, provide local pickup options, and help you find services in your area</string>
		<key>NSLocationWhenInUseUsageDescription</key>
		<string>Sellout uses your location to show nearby marketplace listings, provide local pickup options, and help you find services in your area</string>
		<key>PHPhotoLibraryPreventAutomaticLimitedAccessAlert</key>
		<true/>
		<key>UIApplicationSupportsIndirectInputEvents</key>
		<true/>
		<key>UIBackgroundModes</key>
		<array>
			<string>remote-notification</string>
		</array>
		<key>UILaunchStoryboardName</key>
		<string>LaunchScreen</string>
		<key>UIMainStoryboardFile</key>
		<string>Main</string>
		<key>UIRequiresFullScreen</key>
		<true/>
		<key>UISupportedInterfaceOrientations</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationPortraitUpsideDown</string>
		</array>
		<key>UIStatusBarHidden</key>
		<false/>
	</dict>
</plist>
